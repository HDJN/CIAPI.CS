<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <threshold value="ALL"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[LOG4NET] %date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>

    <root>
      <appender-ref ref="ConsoleAppender"/>
    </root>
  </log4net>
  <appSettings>
    <!---->
    <!--<add key="apiRpcUrl" value="http://ec2-23-20-249-33.compute-1.amazonaws.com:81/api/sky/CIAPI_valid_data/tradingapi"/>-->
   
    <!--<add key="apiRpcUrl" value="http://record.mocument.com:81/sky/ciapisession/ciapi.cityindex.com/tradingapi"/>-->
    
    <!--<add key="apiRpcUrl" value="http://playback.mocument.com:81/sky/ciapisession/ciapi.cityindex.com/tradingapi"/>-->
    
    
    <add key="apiRpcUrl" value="https://ciapi.cityindex.com/tradingapi"/>
    <add key="apiStreamingUrl" value="https://push.cityindex.com"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
</configuration>
